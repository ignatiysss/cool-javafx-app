<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="ua.ignatiysss.cooljavafxapp.controllers.NewMicroObjectController"
      prefHeight="300.0" prefWidth="200.0" fillWidth="false" alignment="CENTER" spacing="20.0">

    <TextField fx:id="text"/>

    <HBox spacing="10">
        <Button defaultButton="true" onAction="#create" text="Create"/>
        <Button cancelButton="true" onAction="#close" text="Cancel"/>
    </HBox>

</VBox>
